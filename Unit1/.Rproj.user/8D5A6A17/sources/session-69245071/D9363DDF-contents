PalmBeach <- read.csv(file.choose())
attach(PalmBeach)
View(PalmBeach)
plot(Buchanan ~ Bush)
abline(lm(Buchanan ~ Bush))
bush.lm <- lm(Buchanan ~ Bush)
library(MASS)
# std resid
plot(Bush, stdres(bush.lm))
abline(h=2, lty=2)
abline(h=-2, lty=2)


stud_resids <- studres(lm(Buchanan ~ Bush))
stud_resids
plot(stud_resids ~ Bush)
abline(h=2, lty=3)
abline(h=-2, lty=3)
outliers <- Filter(function(x) x < -2 || x > 2, stud_resids)
